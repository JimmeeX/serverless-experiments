{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Attach to Lambda",
      "type": "node",
      "request": "attach",
      "protocol": "inspector",
      "port": 5858,
      "localRoot": "${workspaceRoot}",
      "remoteRoot": "/app"
    },
    {
      "name": "Debug Lambda Local",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "program": "./node_modules/.bin/serverless",
      "args": ["offline", "start"],
      "runtimeExecutable": "node",
      "runtimeArgs": ["--inspect"],
      "console": "integratedTerminal"
    }
  ]
}
